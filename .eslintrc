{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "rules": {
    "eol-last": "error",
    "linebreak-style": ["error", "unix"],
    "prefer-const": "error",
    "no-var": "error"
  }
}
